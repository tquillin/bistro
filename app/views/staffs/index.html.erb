
<div class="staffs">

  <div class="header">
      <h1>List of all signed in Staff Members</h1>
  </div>

  <%= form_tag session_path do %>
  <%= label_tag :username %>
    <%= text_field_tag :username %>
    <%= label_tag :password %>
    <%= password_field_tag :password %>
    <%= submit_tag "Log In", class: "btn btn-primary"%>
    <%end%>
    <a href="/staffs/new"> Create New Admin </a><br />

<h1>Logged in staff:<br /><h2> Username: <%= current_staff.username %></h2></h1>

<h3><%= button_to 'Delete' staffs_path method: :delete %></h3>
</div>
