
<div class="staffs">

  <div class="header">
      <h1>List of all signed in Staff Members</h1>
  </div>

  <%= form_tag session_path do %>
  <%= label_tag :username %>
    <%= text_field_tag :username %>
    <%= label_tag :password %>
    <%= password_field_tag :password %>
    <%= submit_tag "Log In", class: "btn btn-primary"%>
    <%end%>
    <a href="/staffs/new"> Create New Staff Member </a>


</div>
